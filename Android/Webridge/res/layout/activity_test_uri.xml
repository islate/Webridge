<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <Button
            android:id="@+id/nativeToJsWithNoParam"
            android:layout_width="300dp"
            android:layout_height="60dp"
            android:layout_gravity="center_horizontal"
            android:text="nativeToJsWithNoParam"
            android:visibility="gone" />

        <Button
            android:id="@+id/nativeToJsWithOneParam"
            android:layout_width="300dp"
            android:layout_height="60dp"
            android:layout_gravity="center_horizontal"
            android:text="nativeToJsWithOneParam"
            android:visibility="gone" />

        <Button
            android:id="@+id/nativeToJsWithMultyParam"
            android:layout_width="300dp"
            android:layout_height="60dp"
            android:layout_gravity="center_horizontal"
            android:text="nativeToJsWithMultyParam"
            android:visibility="gone" />
    </LinearLayout>

    <com.zq.webridge.util.WBWebView
        android:id="@+id/test_uri_wv"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>